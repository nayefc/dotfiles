[user]
        name = Nayef Copty
        email = <%= git_email %>
[alias]
        hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
        histr = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short --all
        ph = push
        pl = pull
        plr = pull --rebase
        gr = remote show origin
        sr = !git branch --set-upstream `git rev-parse --abbrev-ref HEAD` origin/`git rev-parse --abbrev-ref HEAD`
        su = !git push --set-upstream origin `git rev-parse --abbrev-ref HEAD`
        bnuke = !sh -c 'git branch -D $1 && git push origin :$1' -
        tnuke = !sh -c 'git tag -d $1 && git push origin :$1' -
        co = checkout
        subinit = submodule update --init --recursive -f
        g = grep --break --heading --line-number
        st = status -sb
        diffw = diff --color-words
[url "git@git.corp.appnexus.com:"]
        insteadOf = anx:
[push]
        default = simple
[core]
        excludesfile = ~/.gitignore_global
        editor = emacs
[color]
        diff = auto
        status = auto
        interactive = auto
        ui = auto
        pager = true
[color "branch"]
       current = green reverse
       local = blue
       remote = yellow
[color "diff"]
       meta = yellow bold
       frag = magenta bold
       old = red bold
       new = blue bold
[color "status"]
       added = yellow reverse
       changed = blue
       untracked = cyan
[merge]
        conflictstyle = diff3
