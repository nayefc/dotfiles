Protocol 2,1
ForwardAgent yes
KeepAlive yes
StrictHostKeyChecking no
ServerAliveInterval 120
TCPKeepAlive no

Host *
     UseKeychain yes
     AddKeysToAgent yes
     IdentityFile ~/.ssh/id_rsa

Host local
     HostName 127.0.0.1
     Port 8022

Host shsys
     Hostname shsys4

Host sysdb
     HostName shsysdb2
